@model Entidad.Proyecto

@{
    ViewBag.Title = "Detalles";
}

<h2>Detalles del Proyecto</h2>

<div>
    <h4>@Model.NombreProyecto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre del Proyecto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreProyecto)
        </dd>

        <dt>
            Fecha de Inicio
        </dt>

        <dd>
            @Model.FechaInicio.ToShortDateString();
        </dd>

        <dt>
            Fecha FIn
        </dt>

		<dd>
			@Model.FechaFin.ToShortDateString();
		</dd>

    </dl>
</div>
<p>
	<a class="btn btn-primary" href="@Url.Action("Editar","Proyecto",new {id=Model.ProyectoId})" id="btnEditar"> Editar</a>
	<a class="btn btn-primary" href="@Url.Action("Index","Proyecto",new {id=Model.ProyectoId})" id="btnBack">Volver</a>

</p>
